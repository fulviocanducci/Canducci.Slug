<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0;netstandard2.1;</TargetFrameworks>
    <Authors>Canducci</Authors>
    <Company>Canducci</Company>
    <Product>Canducci.Slug</Product>
    <RepositoryUrl>https://github.com/fulviocanducci/Canducci.Slug</RepositoryUrl>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Slug Text</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.4</Version>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/fulviocanducci/Canducci.Slug</PackageProjectUrl>
    <PackageReleaseNotes>- Unlimited text example
- Example with limited text</PackageReleaseNotes>
    <Description>Canducci.Slug</Description>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
